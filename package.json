{
  "name": "backend-developer-test",
  "version": "1.0.0",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "description": "## Welcome!",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --env-file .env ./src/server.ts",
    "start": "tsx watch --env-file ./build/server.js",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/domain/job-posting-management/application/use-cases",
    "test:watch": "vitest --watch --dir src/domain/job-posting-management/application/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/infra/http/controllers",
    "test:e2e:watch": "vitest --dir src/infra/http/controllers",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-sqs": "^3.515.0",
    "@prisma/client": "^5.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.9.1",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "supertest": "^6.3.4",
    "tsx": "^4.7.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  }
}
